{
	"name"        : "enlighteningall/enlighteningall",
	"description" : "Backend Dependency Package List",
	"authors"     : [
		{
			"name"     : "Developer",
			"email"    : "developer@fenclwebdesign.com",
			"homepage" : "https://www.FenclWebDesign.com/",
			"role"     : "Developer"
		}
	],
	"license"     : "MIT",
	"autoload"    : {
		"classmap" : [
			"src/"
		],
		"files"    : [
			"includes/admin/functions.php",
			"includes/configuration.php",
			"includes/functions.php"
		]
	},
	"require"     : {
		"php"                                     : "^8.3",
		"datatables.net/editor-php"               : "^2.0",
		"doctrine/collections"                    : "^1.6",
		"endroid/qr-code"                         : "^4.4",
		"gfaugere/monolog-colored-line"           : "^1.0",
		"google/apiclient"                        : "^2.18",
		"guzzlehttp/guzzle"                       : "^7.9",
		"freelancehunt/php-credit-card-validator" : "^3.2",
		"jenssegers/agent"                        : "^2.6",
		"lorenzo/pinky"                           : "^1.0",
		"matthiasmullie/minify"                   : "^1.3",
		"monolog/monolog"                         : "^2.10",
		"nette/database"                          : "^3.2",
		"nette/forms"                             : "^3.2",
		"nette/utils"                             : "^4.0",
		"nikic/fast-route"                        : "^1.3",
		"omnipay/authorizenet"                    : "^3.3",
		"ozdemir/datatables"                      : "^2.3",
		"pagerfanta/pagerfanta"                   : "^3.1",
		"phpmailer/phpmailer"                     : "^6.1",
		"scssphp/scssphp"                         : "^1.13",
		"tijsverkoyen/css-to-inline-styles"       : "^2.2",
		"twig/intl-extra"                         : "^3.16",
		"twig/twig"                               : "^3.16",
		"xantios/mimey"                           : "^2.2",
		"consoletvs/profanity"                    : "^3.2",
		"jcrowe/bad-word-filter"                  : "^2.2",
		"geoip2/geoip2"                           : "^2.13",
		"rinvex/countries"                        : "^8.1",
		"phpoffice/phpspreadsheet"                : "^1.29",
		"authorizenet/authorizenet"               : "^2.0",
		"php-http/guzzle7-adapter"                : "^1.1",
		"ext-pdo"                                 : "*"
	},
	"scripts"     : {
		"post-update-cmd" : "Google_Task_Composer::cleanup"
	},
	"extra"       : {
		"google/apiclient-services" : [
			"YouTube"
		]
	},
	"config"      : {
		"allow-plugins" : {
			"php-http/discovery" : true
		}
	}
}
